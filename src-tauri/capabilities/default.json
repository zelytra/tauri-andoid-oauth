{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "remote": {
    "urls": ["http://10.0.2.2"]
  },
  "permissions": [
    "core:default",
    "shell:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://10.0.2.2:8080/ping"
        }
      ]
    }
  ]
}
